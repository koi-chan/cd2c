<%= form_with(model: @token_record) do |f| %>
  <div class="form-group">
    <%= f.label(:chat_system_id, for: 'selectChatSystemId') %>
    <%= f.select(:chat_system_id, @chat_systems.map {|c| [c.name, c.id] }, class: 'form-select', required: true, id: 'selectChatSystemId') %>
  </div>

  <div class="form-group">
    <%= f.submit(class: 'btn btn-success') %>
  </div>
<% end %>
