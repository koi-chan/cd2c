<h1><%= @user.username %></h1>
<p>メールアドレス: <%= @user.email %>
<ul>
  <% @user.discord_links.each do |link| %>
    <li><%= link.server_id %> / <%= link.account_id %></li>
  <% end %>
</ul>

<ul>
  <% @user.chat_system_authentication_tokens.each do |token| %>
    <li><%= token.token %></li>
  <% end %>
</ul>
