<h1>オリジナル表一覧</h1>
<p>Find me in app/views/original_tables/index.html.erb</p>

<%= link_to('新規作成', new_original_table_path) %>

<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>作者</th>
      <th>最終更新日</th>
    </tr>
  </thead>
  <tbody>
    <% @original_tables.each do |original_table| %>
      <tr>
        <td><%= link_to(original_table.name, original_table) %></td>
        <td><%= link_to(original_table.user.email) %></td>
        <td><%= original_table.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
